page{
  background-color: #eee;
}
#image-wrapper{
  width: 100%;
  height: 120px;
  background: white;
  margin: 0 auto;
  /* border-radius: 10px; */
  /* border:1px solid #ccc; */
  position: relative;
  font-size: 14px;
  font-family: PingFangSC-Regular;
  font-weight: lighter;
}
#image-wrapper .img{
  width: 100px;
  height: 100px;
  background: red;
  margin: 10px 10px;
}
#image-wrapper .name{
  position: absolute;
  top: 30px;
  left: 35%;
}
#image-wrapper .add{
  position: absolute;
  top: 60px;
  left: 35%;
  font-size: 12px;
  color: #999;
}
#content-wrapper{
  width: 100%;
  height: 212px;
  background-color:#eee;
  margin-top: 10px;
}
#content-wrapper view{
  background-color: white;
  margin-bottom: 3px;
  height: 40px;
  position: relative;
}
#content-wrapper view view{
  font-size: 14px;
  font-family:PingFangSC-Regular;
  line-height: 40px;
  margin-left: 20px;
  color: #999;
  font-weight: lighter;
}
#content-wrapper .room-name-value{
  position: absolute;
  top: 0;
  left: 70%;
}